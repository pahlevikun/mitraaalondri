<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="0dp"
        android:bottom="0dp">
        <shape
            android:dither="true"
            android:shape="rectangle">
            <corners
                android:radius="0dp"/>
            <solid
                android:color="@color/colorGrey" />
            <stroke
                android:width="1dp"
                android:color="@color/colorGrey"/>
        </shape>
    </item>
</layer-list>

